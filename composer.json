{
    "name": "rcsv/oreno-url-shortener",
    "type": "project",
    "description": "A simple URL shortener built with Slim 4",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "slim/psr7": "^1.6",
        "slim/slim": "^4.11",
        "slim/twig-view": "^3.3",
        "php-di/php-di": "^7.0",
        "vlucas/phpdotenv": "^5.5",
        "jenssegers/blade": "^1.1",
        "slim/csrf": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1",
        "monolog/monolog": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "Rcsvpg\\Murls\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Rcsvpg",
            "email": "rcsvpg@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
